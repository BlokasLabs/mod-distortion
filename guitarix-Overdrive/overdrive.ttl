@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix epp:  <http://lv2plug.in/ns/ext/port-props#> .
@prefix ev:   <http://lv2plug.in/ns/ext/event#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix mod: <http://moddevices.com/ns/modgui#>.

<http://moddevices.com/plugins/guitarix/overdrive>
       a lv2:Plugin , lv2:DistortionPlugin;
       doap:name "Gx Overdrive" ;
       lv2:binary <overdrive.so> ;
       lv2:optionalFeature epp:supportsStrictBounds ;
       lv2:optionalFeature lv2:hardRtCapable ;
    
    lv2:minorVersion 0;
    lv2:microVersion 8;
       
    lv2:port [
	a lv2:ControlPort ;
	a lv2:InputPort ;
	lv2:index 0 ;
	lv2:symbol "drive" ;
	lv2:name "drive" ;
	lv2:default 1 ;
	lv2:minimum 1 ;
	lv2:maximum 20 ;
    ] , [
	a lv2:ControlPort ;
	a lv2:InputPort ;
	lv2:index 1 ;
	lv2:symbol "dry_wet" ;
	lv2:name "dry/wet" ;
	lv2:default 100 ;
	lv2:minimum 0 ;
	lv2:maximum 100 ;
    ] , [
	a lv2:AudioPort ;
	a lv2:InputPort ;
	lv2:index 2 ;
	lv2:symbol "in0" ;
	lv2:name "in0" ;
    ] , [
	a lv2:AudioPort ;
	a lv2:OutputPort ;
	lv2:index 3 ;
	lv2:symbol "out0" ;
	lv2:name "out0" ;
    ];
    mod:gui [
        a mod:Gui;
        mod:resourcesDirectory <modgui>;
        mod:iconTemplate <modgui/icon-gx-overdrive.html>;
        mod:templateData <modgui/data-gx-overdrive.json>;
        mod:screenshot <modgui/screenshot-gx-overdrive.png>;
        mod:thumbnail <modgui/thumb-gx-overdrive.png>;
    ] .